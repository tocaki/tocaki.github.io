{% capture json %}{% for file in site.static_files %}{% if file.path contains '/images/artists' %}
  {
    "path": "{{ file.path }}",
    "name": "{{ file.path | replace: "/images/artists/", "" }}"
  },
{% endif %}{% endfor %}{% endcapture %}
avatars([
  {{ json | strip | split: ',' | join: ',' }}
])
